    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^4.0.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.0.1",
    "element-ui": "^2.4.11",
    "imagemin": "^6.0.0",
    "jquery": "^3.2",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.24.0",
